#:kivy 2.1.0
    
<CurialeGUI>:
    do_default_tab: True
    orientation: 'vertical'
    TabbedPanelItem:
        text: 'Text Controll'
        BoxLayout:
            orientation: 'horizontal'
            TabbedPanelItem:
                text: 'FILE'
                BoxLayout:
                    orientation: 'vertical'
                    TextInput:
                        id: pathWords
                        hint_text:'Enter path file words'
            TabbedPanelItem:
                text: 'LIST'
                BoxLayout:
                    orientation: 'vertical'
                    TextInput:
                        id: pathWords
                        hint_text:'Enter List words'
            TabbedPanelItem:
                text: 'URL'
                BoxLayout:
                    orientation: 'vertical'
                    TextInput:
                        id: pathWords
                        hint_text:'Enter path url words'
        
        

    TabbedPanelItem:
        text: 'Image Controll'

        BoxLayout:
            padding: 50, 50, 50, 50
            orientation: 'horizontal'

            BoxLayout:
                spacing: 50
                orientation: 'vertical'
                Label:
                    text: 'label'
                Label:
                    text: 'label'
                Label:

            BoxLayout:
                spacing: 50
                orientation: 'vertical'
                TextInput:
                    text: 'TextInput'
                TextInput:
                    text: 'TextInput'
                Button:
                    spacing: 100
                    text: 'button'